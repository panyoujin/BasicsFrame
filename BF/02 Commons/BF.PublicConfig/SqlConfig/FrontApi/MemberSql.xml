<?xml version="1.0" encoding="utf-8" ?>
<HCIP.AjaxDataSetting>

  <Data name="FrontApi_GetMemberInfoByAccount">
    <SqlDefinition type="sql">
      <SqlCommand>
        <![CDATA[
SELECT `Account`,`Passwd`,`Name`,`Age`,`Birthday`,`Sex`,`Email`,`Phone`,`Remark`,`QQ`,`ImageUrl` FROM `TB_MB_MemberInfo` WHERE `Account`='@@Account@@' AND `Status`=FALSE;
      ]]>
      </SqlCommand>
    </SqlDefinition>
  </Data>

</HCIP.AjaxDataSetting>